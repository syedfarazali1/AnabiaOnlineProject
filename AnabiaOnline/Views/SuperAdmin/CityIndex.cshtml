@model IEnumerable<AnabiaOnline.Models.City>

@{
    ViewBag.Title = "CityIndex";
    Layout = "~/Views/Shared/dashboard.cshtml";
}

<h2>CityIndex</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table" id="datatables">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Cit_Name)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Cit_Name)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.CityId }) |
            @Html.ActionLink("Details", "Details", new { id=item.CityId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.CityId })
        </td>
    </tr>
}

</table>
