



<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta content="width=device-width, initial-scale=1.0" name="viewport">
    @if (@Session["StoreloginID"] != null)
    {
        <title>Dashboard - @Session["StoreBussniessName"]</title>
    }
    else
    {
        <title>Dashboard - AnabiaOnline</title>
    }


    <meta content="" name="description">
    <meta content="" name="keywords">

    <!-- Favicons -->
    <link href="~/assets/img/favicon.png" rel="icon">
    <link href="~/assets/img/apple-touch-icon.png" rel="apple-touch-icon">

    <!-- Google Fonts -->
    <link href="https://fonts.gstatic.com" rel="preconnect">
    <link href="https://fonts.googleapis.com/css?family=Open+Sans:300,300i,400,400i,600,600i,700,700i|Nunito:300,300i,400,400i,600,600i,700,700i|Poppins:300,300i,400,400i,500,500i,600,600i,700,700i" rel="stylesheet">

    <!-- Vendor CSS Files -->
    <link href="~/assets/vendor/bootstrap/css/bootstrap.min.css" rel="stylesheet">
    <link href="~/assets/vendor/bootstrap-icons/bootstrap-icons.css" rel="stylesheet">
    <link href="~/assets/vendor/boxicons/css/boxicons.min.css" rel="stylesheet">
    <link href="~/assets/vendor/quill/quill.snow.css" rel="stylesheet">
    <link href="~/assets/vendor/quill/quill.bubble.css" rel="stylesheet">
    <link href="~/assets/vendor/remixicon/remixicon.css" rel="stylesheet">
    <link href="~/assets/vendor/simple-datatables/style.css" rel="stylesheet">

    <!-- Datatable Files -->
    <!-- Template Main CSS File -->
    <link href="~/assets/css/style.css" rel="stylesheet">

    <!-- =======================================================
    * Template Name: NiceAdmin - v2.4.1
    * Template URL: https://bootstrapmade.com/nice-admin-bootstrap-admin-html-template/
    * Author: BootstrapMade.com
    * License: https://bootstrapmade.com/license/
    ======================================================== -->
</head>

<body>

    <!-- ======= Header ======= -->
    <header id="header" class="header fixed-top d-flex align-items-center">

        <div class="d-flex align-items-center justify-content-between">
            <a href="" class="logo d-flex align-items-center">
                <img src="~/assets/img/logo.png" alt="">
                
                    @if (@Session["StoreloginID"] != null)
                    {
            <span class="d-none d-lg-block">  @Session["StoreBussniessName"]</span>
                    }
                    else
                    {
                         <span class="d-none d-lg-block">AnabiaOnline
                    </span>
                }
                    
                </a>
            <i class="bi bi-list toggle-sidebar-btn"></i>
        </div><!-- End Logo -->

        

        <nav class="header-nav ms-auto">
            <ul class="d-flex align-items-center">

                    <li class="nav-item d-block d-lg-none">
                        <a class="nav-link nav-icon search-bar-toggle " href="#">
                            <i class="bi bi-search"></i>
                        </a>
                    </li><!-- End Search Icon-->

                <li class="nav-item dropdown pe-3">

                    <a class="nav-link nav-profile d-flex align-items-center pe-0" href="#" data-bs-toggle="dropdown">


                        @if (@Session["StoreloginID"] != null)
                        {
                            <span class="d-none d-md-block dropdown-toggle ps-2">
                                @Session["StoreBussniessName"]
                            </span>
                        }
                        else
                        {
                            <span class="d-none d-md-block dropdown-toggle ps-2">
                                AnabiaOnline
                            </span>
                        }

                    </a><!-- End Profile Iamge Icon -->

                    <ul class="dropdown-menu dropdown-menu-end dropdown-menu-arrow profile">
                        <li class="dropdown-header">
                            @if (@Session["StoreloginID"] != null)
                            {
                                <h6>@Session["StoreBussniessName"]</h6>
                            }
                            else
                            {
                                <h6>AnabiaOnline</h6>
                            }


                        </li>
                        <li>
                            <hr class="dropdown-divider">
                        </li>

                     
                        <li>
                            <hr class="dropdown-divider">
                        </li>

                      
                        <li>
                            <hr class="dropdown-divider">
                        </li>

                        <li>

                      
                            @if (Session["StoreloginID"] != null )
                            {
                                <a class="dropdown-item d-flex align-items-center" href="~/Store/Signout">

                                    <i class="bi bi-box-arrow-right"></i>

                                    <span>Sign Out</span>
                                </a>
                            }
                            else if (Session["Userid"] != null && Session["UserName"] != null)
                            {
                                <a class="dropdown-item d-flex align-items-center" href="~/SuperAdmin/Signout">

                                    <i class="bi bi-box-arrow-right"></i>

                                    <span>Sign Out</span>
                                </a>
                            }



                        </li>

                    </ul><!-- End Profile Dropdown Items -->
                </li><!-- End Profile Nav -->

            </ul>
        </nav><!-- End Icons Navigation -->

    </header><!-- End Header -->
    <!-- ======= Sidebar ======= -->
    <aside id="sidebar" class="sidebar">

        <ul class="sidebar-nav" id="sidebar-nav">

            <li class="nav-item">
                <a class="nav-link " href="">
                    <i class="bi bi-grid"></i>
                    <span>Dashboard</span>
                </a>
            </li><!-- End Dashboard Nav -->




            <li class="nav-heading">Pages</li>
            @if (Session["StoreloginID"] != null && Session["StoreBussniessName"] != null)
            {
                <li class="nav-item">
                    <a class="nav-link collapsed" href="~/Store/CategoryList">
                        <i class="bi bi-layout-text-window-reverse"></i>
                        <span>Category</span>
                    </a>
                </li>

                <li class="nav-item">
                    <a class="nav-link collapsed" href="~/Store/ProductList">
                        <i class="bi bi-bar-chart"></i>
                        <span>Product</span>
                    </a>
                </li>
                <li class="nav-item">
                    <a class="nav-link collapsed" href="~/Store/LogoIndex">
                        <i class="bi bi-bar-chart"></i>
                        <span>Brands</span>
                    </a>
                </li>

                <li class="nav-item">
                    <a class="nav-link collapsed" href="~/Store/OrderList">
                        <i class="bi bi-journal-text"></i>
                        <span>Order List</span>
                    </a>
                </li>

                <li class="nav-item">
                    <a class="nav-link collapsed" href="~/Store/WebstoreIndex">
                        <i class="bi bi-gem"></i>
                        <span>Web Store</span>
                    </a>
                </li>
                <li class="nav-item">
                    <a class="nav-link collapsed" href="~/Store/DispatchList">
                        <i class="bi bi-envelope"></i>
                        <span>Dispatch List</span>
                    </a>
                </li>
            }
            @if (Session["Userid"] != null && Session["UserName"] != null)
            {
                <li class="nav-item">
                    <a class="nav-link collapsed" href="~/SuperAdmin/StoresListbypanding">
                        <i class="bi bi-card-list"></i>
                        <span>Resgistration Request</span>
                    </a>
                </li>
                <li class="nav-item">
                    <a class="nav-link collapsed" href="~/SuperAdmin/StoresListbyapproved">
                        <i class="bi bi-cart"></i>
                        <span>Shops List</span>
                    </a>
                </li><!-- End Profile Page Nav -->
            }

            <!-- End Contact Page Nav -->


            <li class="nav-item">


                @if (Session["StoreloginID"] != null )
                {
                    <a class="nav-link collapsed" href="~/Store/Signout">

                        <i class="bi bi-box-arrow-in-right"></i>

                        <span>Sign Out</span>
                    </a>
                }
                else if (Session["Userid"] != null && Session["UserName"] != null)
                {
                    <a class="nav-link collapsed" href="~/SuperAdmin/Signout">

                        <i class="bi bi-box-arrow-in-right"></i>
                        <span>Sign Out</span>
                    </a>
                }



            </li><!-- End Login Page Nav -->





        </ul>

    </aside><!-- End Sidebar-->

    <main id="main" class="main">

        <div class="container-fluid">
            @RenderBody()

        </div>

    </main><!-- End #main -->
    <!-- ======= Footer ======= -->


    <footer id="footer" class="footer">
        <div class="copyright">
            &copy; Copyright <strong><span>AnabiaOnline</span></strong>. All Rights Reserved
        </div>
        <div class="credits">
            
            Designed by <a href="https://www.w3bcreatorz.com">W3B Creatorz</a>
                       
        </div>
    </footer><!-- End Footer -->

    <a href="#" class="back-to-top d-flex align-items-center justify-content-center"><i class="bi bi-arrow-up-short"></i></a>

    <!-- Vendor JS Files -->
    <script src="~/assets/vendor/apexcharts/apexcharts.min.js"></script>
    <script src="~/assets/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>
    <script src="~/assets/vendor/chart.js/chart.min.js"></script>
    <script src="~/assets/vendor/echarts/echarts.min.js"></script>
    <script src="~/assets/vendor/quill/quill.min.js"></script>
  
    <script src="~/assets/vendor/tinymce/tinymce.min.js"></script>
    <script src="~/assets/vendor/php-email-form/validate.js"></script>

    <!-- Template Main JS File -->
    <script src="~/assets/js/main.js"></script>
    <link href="~/datatable/jquery.dataTables.min.css" rel="stylesheet" />

    <script src="~/datatable/jquery.dataTables.min.js"></script>

    

    <script>


        $(document).ready(function () {
            $('#datatables').DataTable();
        });
    </script>

</body>

</html>



